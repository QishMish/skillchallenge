{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@app/auth": [
        "libs/auth/src"
      ],
      "@app/auth/*": [
        "libs/auth/src/*"
      ],
      "@app/types": [
        "libs/types/src"
      ],
      "@app/types/*": [
        "libs/types/src/*"
      ],
      "@app/users": [
        "libs/users/src"
      ],
      "@app/users/*": [
        "libs/users/src/*"
      ],
      "@app/database": [
        "libs/database/src"
      ],
      "@app/database/*": [
        "libs/database/src/*"
      ],
      "@app/utils": [
        "libs/utils/src"
      ],
      "@app/utils/*": [
        "libs/utils/src/*"
      ],
      "@app/mail": [
        "libs/mail/src"
      ],
      "@app/mail/*": [
        "libs/mail/src/*"
      ],
      "@app/rmq": [
        "libs/rmq/src"
      ],
      "@app/rmq/*": [
        "libs/rmq/src/*"
      ],
      "@app/health-check": [
        "libs/health-check/src"
      ],
      "@app/health-check/*": [
        "libs/health-check/src/*"
      ],
      "@app/token": [
        "libs/token/src"
      ],
      "@app/token/*": [
        "libs/token/src/*"
      ],
      "@app/skill-test": [
        "libs/skill-test/src"
      ],
      "@app/skill-test/*": [
        "libs/skill-test/src/*"
      ],
      "@app/question": [
        "libs/question/src"
      ],
      "@app/question/*": [
        "libs/question/src/*"
      ],
      "@app/option": [
        "libs/option/src"
      ],
      "@app/option/*": [
        "libs/option/src/*"
      ],
      "@app/media": [
        "libs/media/src"
      ],
      "@app/media/*": [
        "libs/media/src/*"
      ],
      "@app/common": [
        "libs/common/src"
      ],
      "@app/common/*": [
        "libs/common/src/*"
      ],
      "@app/auth-rpc": [
        "libs/auth-rpc/src"
      ],
      "@app/auth-rpc/*": [
        "libs/auth-rpc/src/*"
      ]
    }
  }
}