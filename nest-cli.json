{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/skillchallenge/src",
  "monorepo": true,
  "root": "apps/skillchallenge",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/skillchallenge/tsconfig.app.json"
  },
  "projects": {
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "analytics": {
      "type": "application",
      "root": "apps/analytics",
      "entryFile": "main",
      "sourceRoot": "apps/analytics/src",
      "compilerOptions": {
        "tsConfigPath": "apps/analytics/tsconfig.app.json"
      }
    },
    "mailer": {
      "type": "application",
      "root": "apps/mailer",
      "entryFile": "main",
      "sourceRoot": "apps/mailer/src",
      "compilerOptions": {
        "tsConfigPath": "apps/mailer/tsconfig.app.json"
      }
    },
    "rmq": {
      "type": "library",
      "root": "libs/rmq",
      "entryFile": "index",
      "sourceRoot": "libs/rmq/src",
      "compilerOptions": {
        "tsConfigPath": "libs/rmq/tsconfig.lib.json"
      }
    },
    "health-check": {
      "type": "library",
      "root": "libs/health-check",
      "entryFile": "index",
      "sourceRoot": "libs/health-check/src",
      "compilerOptions": {
        "tsConfigPath": "libs/health-check/tsconfig.lib.json"
      }
    }
  }
}